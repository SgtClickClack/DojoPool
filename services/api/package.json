{
  "name": "@dojopool/api",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.9.3",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev": "yarn run start:dev",
    "postinstall": "prisma generate --schema=../../packages/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=../../packages/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=../../packages/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=../../packages/prisma/schema.prisma",
    "seed:achievements": "ts-node src/achievements/achievements.seed.ts",
    "test:websocket": "node src/__tests__/run-websocket-tests.js",
    "test:websocket:chat": "npx jest --testPathPattern=chat-websocket.integration.spec.ts",
    "test:websocket:activity": "npx jest --testPathPattern=activity-events-websocket.integration.spec.ts",
    "test:websocket:general": "npx jest --testPathPattern=websocket-general.integration.spec.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "worker:prod": "node dist/worker.js",
    "test:queue": "node test-queue-integration.js",
    "test:edge-cache": "node test-edge-cache.js",
    "generate:docs": "node scripts/generate-openapi.js",
    "docs:serve": "npx http-server docs -p 8080",
    "lint": "eslint src/ --ext .ts --max-warnings 0",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.7",
    "@bull-board/express": "^6.12.7",
    "@bull-board/ui": "^6.12.7",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "6.6.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@techstark/opencv-js": "4.11.0-release.1",
    "@types/multer": "^2.0.0",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^1.41.3",
    "dompurify": "^3.2.4",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "jpeg-js": "^0.4.4",
    "jsdom": "^26.0.0",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pngjs": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "socket.io-adapter": "^2.5.4",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.19.0",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.21",
    "postcss": "8.5.6",
    "prisma": "6.6.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
