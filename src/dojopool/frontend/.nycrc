{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": ["text", "html", "lcov"],
  "statements": 80,
  "branches": 75,
  "functions": 80,
  "lines": 80,
  "exclude": [
    "coverage/**",
    "cypress/**",
    "**/*.test.{js,ts,jsx,tsx}",
    "**/*.spec.{js,ts,jsx,tsx}",
    "**/*.d.ts",
    "**/node_modules/**"
  ]
}
