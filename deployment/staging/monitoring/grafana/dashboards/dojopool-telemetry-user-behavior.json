{
  "dashboard": {
    "id": null,
    "title": "DojoPool User Behavior Telemetry",
    "tags": ["dojopool", "telemetry", "user-behavior", "analytics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Real-Time User Activity",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(dojopool_user_activity_total[5m]) * 60",
            "legendFormat": "Activities/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Users by Time",
        "type": "stat",
        "targets": [
          {
            "expr": "dojopool_active_users_current",
            "legendFormat": "Currently Active"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "New User Registrations (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(dojopool_users_registered_total[24h])",
            "legendFormat": "New Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Average Session Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "dojopool_avg_session_duration_minutes",
            "legendFormat": "Avg Session (min)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "m",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 5
                },
                {
                  "color": "green",
                  "value": 15
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "User Activity Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(dojopool_user_activity_total[5m])",
            "legendFormat": "Activity Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 6,
        "title": "User Engagement by Hour",
        "type": "barchart",
        "targets": [
          {
            "expr": "increase(dojopool_user_activity_total[1h])",
            "legendFormat": "Activities per hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 7,
        "title": "User Journey Completion Rates",
        "type": "barchart",
        "targets": [
          {
            "expr": "dojopool_journey_completion_rate{step=\"signup\"}",
            "legendFormat": "Signup"
          },
          {
            "expr": "dojopool_journey_completion_rate{step=\"onboarding\"}",
            "legendFormat": "Onboarding"
          },
          {
            "expr": "dojopool_journey_completion_rate{step=\"first_game\"}",
            "legendFormat": "First Game"
          },
          {
            "expr": "dojopool_journey_completion_rate{step=\"returning\"}",
            "legendFormat": "Returning User"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Top User Actions",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(dojopool_user_action_total[5m]))",
            "legendFormat": "{{action}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 9,
        "title": "User Segmentation",
