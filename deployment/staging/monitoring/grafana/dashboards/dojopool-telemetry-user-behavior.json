        "targets": [
          {
            "expr": "dojopool_user_segment_total",
            "legendFormat": "{{segment}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Feature Adoption Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(dojopool_feature_usage_total{feature=\"venue_checkin\"}[5m]) * 60",
            "legendFormat": "Venue Check-ins"
          },
          {
            "expr": "rate(dojopool_feature_usage_total{feature=\"tournament_join\"}[5m]) * 60",
            "legendFormat": "Tournament Joins"
          },
          {
            "expr": "rate(dojopool_feature_usage_total{feature=\"social_interaction\"}[5m]) * 60",
            "legendFormat": "Social Interactions"
          },
          {
            "expr": "rate(dojopool_feature_usage_total{feature=\"marketplace\"}[5m]) * 60",
            "legendFormat": "Marketplace Usage"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        }
      },
      {
        "id": 11,
        "title": "User Retention Trends",
        "type": "graph",
        "targets": [
          {
            "expr": "dojopool_user_retention_1d",
            "legendFormat": "Day 1 Retention"
          },
          {
            "expr": "dojopool_user_retention_7d",
            "legendFormat": "Day 7 Retention"
          },
          {
            "expr": "dojopool_user_retention_30d",
            "legendFormat": "Day 30 Retention"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 38
        }
      },
      {
        "id": 12,
        "title": "Geographic User Distribution",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, dojopool_users_by_region)",
            "legendFormat": "{{region}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 38
        }
      },
      {
        "id": 13,
        "title": "Device and Browser Analytics",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, dojopool_user_device_total)",
            "legendFormat": "{{device_type}} - {{browser}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 46
        }
      },
      {
        "id": 14,
        "title": "Error and Friction Points",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(dojopool_user_error_total[5m]) * 60",
            "legendFormat": "User Errors"
          },
          {
            "expr": "rate(dojopool_user_friction_total{type=\"onboarding_dropoff\"}[5m]) * 60",
            "legendFormat": "Onboarding Drop-offs"
          },
          {
            "expr": "rate(dojopool_user_friction_total{type=\"feature_confusion\"}[5m]) * 60",
            "legendFormat": "Feature Confusion"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 46
        }
      }
    ],
    "time": {
      "from": "now-24h",
