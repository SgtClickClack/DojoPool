{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": ".next",
  "installCommand": "yarn install --immutable --frozen-lockfile",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/pages/api/**/*.js": {
      "maxDuration": 30
    },
    "src/pages/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
