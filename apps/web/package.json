{
  "name": "dojopool-frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.9.3",
  "scripts": {
    "dev": "next dev",
    "dev:log": "powershell -Command \"$timestamp = Get-Date -Format 'yyyy-MM-dd_HH-mm-ss'; $logFile = 'crash-log-$timestamp.txt'; Write-Host 'Starting server with crash logging...'; Write-Host 'Log file: $logFile'; Write-Host 'Press Ctrl+C to stop'; yarn run dev 2>&1 | Tee-Object -FilePath $logFile; Write-Host 'Server stopped. Check $logFile for crash details.'\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "analyze": "node scripts/analyze-bundle.js",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@heroicons/react": "2.2.0",
    "@mui/icons-material": "5.15.0",
    "@mui/material": "5.15.0",
    "@mui/system": "5.15.0",
    "@mui/x-date-pickers": "^6.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@react-google-maps/api": "^2.20.7",
    "@vis.gl/react-maplibre": "^8.0.4",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "critters": "^0.0.25",
    "cypress": "^14.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.12",
    "maplibre-gl": "^5.7.0",
    "next": "14.2.32",
    "next-auth": "^4.24.11",
    "notistack": "3.0.2",
    "quill-delta": "^5.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-image-crop": "^11.0.10",
    "react-quill-new": "^3.6.0",
    "react-router-dom": "^6.11.1",
    "socket.io-client": "^4.6.1",
    "swr": "^2.2.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.1.3",
    "@types/react": "18.3.0",
    "@types/react-dom": "18.3.1",
    "@types/react-image-crop": "^8.1.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  }
}
