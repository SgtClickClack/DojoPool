{
  "version": 2,
  "buildCommand": "cd apps/web && yarn build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "yarn install --frozen-lockfile && npx prisma generate",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "apps/web/.next/server/pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
