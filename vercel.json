{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/web && yarn install --immutable && yarn type-check && yarn build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "yarn install --immutable",
  "framework": "nextjs",
  "functions": {
    "apps/web/next.config.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
