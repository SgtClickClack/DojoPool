{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "services/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/web/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "services/api/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web"
    }
  ]
}
