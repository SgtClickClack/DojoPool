[run]
source = src/core/matchmaking
omit =
    src/core/matchmaking/tests/*
    src/core/matchmaking/__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    raise AssertionError

[html]
directory = coverage_html_report
