{
  "name": "dojopool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run optimize:assets && next build",
    "start": "node server.js",
    "lint": "next lint",
    "fix": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:update": "jest -u",
    "test:clear": "jest --clearCache",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:component": "cypress run --component",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cypress:run",
    "test:all": "npm run test && npm run test:e2e:ci",
    "lighthouse": "lhci autorun",
    "test:perf": "npm run build && npm run lighthouse",
    "test:a11y": "pa11y-ci './src/pages/**/*.tsx'",
    "prepare": "npx husky install",
    "optimize:db": "ts-node scripts/optimize-database.ts",
    "optimize:db:prod": "NODE_ENV=production ts-node scripts/optimize-database.ts",
    "optimize:assets": "ts-node scripts/optimize-assets.ts",
    "optimize:assets:prod": "NODE_ENV=production ts-node scripts/optimize-assets.ts",
    "test:load": "k6 run src/tests/performance/load-test.js",
    "test:load:ci": "k6 run --out json=load-test-results.json src/tests/performance/load-test.js",
    "test:load:cloud": "k6 cloud src/tests/performance/load-test.js",
    "test:perf:all": "npm run test:perf && npm run test:load",
    "test:scalability": "ts-node src/tests/scalability/runner.ts",
    "test:scalability:load": "ts-node src/tests/scalability/runner.ts --scenario=loadTest",
    "test:scalability:stress": "ts-node src/tests/scalability/runner.ts --scenario=stressTest",
    "test:scalability:spike": "ts-node src/tests/scalability/runner.ts --scenario=spikeTest",
    "test:scalability:endurance": "ts-node src/tests/scalability/runner.ts --scenario=enduranceTest",
    "test:scalability:ci": "ts-node src/tests/scalability/runner.ts --ci --out=scalability-results.json",
    "test:performance": "cypress run --spec \"cypress/e2e/performance/**/*.cy.ts\"",
    "report:performance": "node scripts/generate-performance-report.js"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.0.0",
    "@aws-sdk/client-sns": "^3.0.0",
    "@clickhouse/client": "^0.2.10",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@logtail/node": "^0.4.21",
    "@mui/icons-material": "5.16.14",
    "@mui/material": "5.16.14",
    "@segment/analytics-node": "^2.0.0",
    "@tensorflow/tfjs": "4.22.0",
    "@tensorflow/tfjs-backend-webgl": "4.22.0",
    "connect-redis": "^7.1.1",
    "csurf": "^1.11.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "firebase": "11.2.0",
    "geoip-lite": "^1.4.9",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mixpanel": "^0.18.0",
    "msw": "^2.1.5",
    "next": "^15.1.6",
    "rate-limit-redis": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-websocket": "4.12.0",
    "request-ip": "^3.3.0",
    "ua-parser-js": "^1.0.37",
    "uuid": "11.0.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.9",
    "@babel/preset-typescript": "^7.23.9",
    "@babel/runtime": "7.26.7",
    "@cypress/code-coverage": "3.13.11",
    "@lhci/cli": "0.14.0",
    "@testing-library/cypress": "10.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/csurf": "^1.11.5",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.10",
    "@types/geoip-lite": "^1.4.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/request-ip": "^0.0.41",
    "@types/ua-parser-js": "^0.7.39",
    "babel-jest": "^29.7.0",
    "cypress": "14.0.1",
    "eslint": "9.19.0",
    "eslint-config-next": "15.1.6",
    "glob-promise": "^6.0.5",
    "husky": "^8.0.0",
    "image-webpack-loader": "^8.1.0",
    "istanbul-lib-coverage": "3.2.2",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "6.4.0",
    "k6": "0.0.0",
    "nyc": "17.1.0",
    "sharp": "^0.32.6",
    "start-server-and-test": "2.0.10",
    "ts-jest": "^29.1.2",
    "typescript": "5.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --findRelatedTests --passWithNoTests"
    ]
  }
}
