{
  "name": "dojopool",
  "version": "1.0.0",
  "description": "DojoPool Platform",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:frontend": "cd apps/web && npm run dev",
    "dev:frontend:3001": "cd apps/web && next dev -p 3001",
    "dev:frontend:3004": "cd apps/web && next dev -p 3004",
    "dev:frontend:3011": "cd apps/web && next dev -p 3011",
    "dev:web:3000": "next dev -p 3000",
    "dev:next": "next dev -p 3001",
    "build:backend": "tsc -p tsconfig.backend.json",
    "start:backend": "node dist/backend/index.js",
    "start": "npx ts-node --esm src/backend/index.ts",
    "dev:backend": "cd services/api && set PORT=3002 && npm run start:dev",
    "build": "next build",
    "preview": "npm run env:check:strict && next start",
    "test": "vitest run --config vitest.unit.config.ts",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --config vitest.unit.config.ts --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:update": "vitest -u",
    "test:debug": "vitest --inspect-brk",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:unit:coverage": "vitest run --config vitest.unit.config.ts --coverage",
    "test:int": "vitest run --config vitest.integration.config.ts",
    "test:int:coverage": "vitest run --config vitest.integration.config.ts --coverage",
    "test:ci:js": "npm run test:unit:coverage && npm run test:int:coverage",
    "test:all": "vitest run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "cypress:run:venues": "cypress run --spec \"cypress/e2e/venue-discovery-test.cy.ts\" --config baseUrl=http://localhost:3011",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:venues": "start-server-and-test dev:frontend:3011 http://localhost:3011 cypress:run:venues",
    "benchmark": "ts-node src/tests/benchmarks/NetworkTransport.bench.ts",
    "validate-firebase-config": "node scripts/validate-firebase-config.js",
    "validate-firebase-config:fix": "node scripts/validate-firebase-config.js --fix",
    "validate-firebase-config:fetch": "node scripts/validate-firebase-config.js --fetch",
    "setup-env": "node scripts/setup-env.js",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "npm run type-check:web && npm run type-check:api",
    "type-check:web": "tsc -p apps/web/tsconfig.json --noEmit",
    "type-check:api": "tsc -p services/api/tsconfig.json --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ai:explain-error": "node scripts/ai/explain-error.js",
    "ai:fix": "node scripts/ai/fix-with-ai.js",
    "ai:refactor": "node scripts/ai/refactor.js",
    "ai:convert": "node scripts/ai/convert.js",
    "ai:bug-repro": "node scripts/ai/bug-repro.js",
    "ai:commit": "node scripts/ai/generate-commit-msg.js",
    "ai:pr": "node scripts/ai/generate-pr-description.js",
    "ai:test:summary": "node scripts/ai/summarize-tests.js",
    "ai:review": "node scripts/ai/review-changes.js",
    "ai:detect": "node scripts/ai/detect-errors.js",
    "qodana:scan": "qodana scan",
    "qodana:ui": "qodana scan --show-report",
    "cleanup:node:win": "powershell -ExecutionPolicy Bypass -File scripts\\cleanup-node-processes.ps1",
    "dev:capture": "powershell -ExecutionPolicy Bypass -File scripts\\dev-capture.ps1",
    "dev:capture:clean": "npm run cleanup:node:win && npm run dev:capture",
    "tasks:sync": "node scripts/tasks-sync.mjs",
    "env:check": "node scripts/validate-env.mjs",
    "env:check:strict": "node scripts/validate-env.mjs --strict",
    "report:duplicates": "node scripts/report-duplicates.mjs",
    "prepare": "husky"
  },
