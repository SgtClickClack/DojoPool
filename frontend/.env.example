# Frontend Environment Configuration

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080/api
NEXT_PUBLIC_WS_URL=ws://localhost:8080/ws

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=auth.dojopool.com
NEXT_PUBLIC_CLIENT_ID=your-client-id
NEXT_PUBLIC_AUDIENCE=https://api.dojopool.com

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_SENTRY=false
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Game Configuration
NEXT_PUBLIC_MAX_PLAYERS=2
NEXT_PUBLIC_GAME_TIMEOUT=3600
NEXT_PUBLIC_MATCHMAKING_TIMEOUT=300

# External Services
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
NEXT_PUBLIC_BLOCKCHAIN_ENABLED=false
NEXT_PUBLIC_BLOCKCHAIN_NETWORK=testnet

# Analytics & Monitoring
NEXT_PUBLIC_GA_TRACKING_ID=UA-XXXXXXXXX-X
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# UI Configuration
NEXT_PUBLIC_THEME=light
NEXT_PUBLIC_PRIMARY_COLOR=#007AFF
NEXT_PUBLIC_SECONDARY_COLOR=#5856D6

# Social Media
NEXT_PUBLIC_TWITTER_HANDLE=@dojopool
NEXT_PUBLIC_DISCORD_INVITE=https://discord.gg/dojopool

# Deployment
NEXT_PUBLIC_ENVIRONMENT=development
NEXT_PUBLIC_VERSION=1.0.0
NEXT_PUBLIC_BUILD_TIME=2024-02-19T12:00:00Z

# Content Delivery
NEXT_PUBLIC_CDN_URL=https://cdn.dojopool.com
NEXT_PUBLIC_ASSETS_URL=https://assets.dojopool.com

# Internationalization
NEXT_PUBLIC_DEFAULT_LOCALE=en
NEXT_PUBLIC_SUPPORTED_LOCALES=en,es,fr,de,ja

# Performance
NEXT_PUBLIC_IMAGE_OPTIMIZATION=true
NEXT_PUBLIC_CACHE_MAX_AGE=3600
NEXT_PUBLIC_COMPRESSION_ENABLED=true

# Debug
NEXT_PUBLIC_DEBUG_MODE=true
NEXT_PUBLIC_LOG_LEVEL=debug
NEXT_PUBLIC_ENABLE_REDUX_DEVTOOLS=true 