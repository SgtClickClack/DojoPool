    "@mui/x-date-pickers": "^6.19.0",
    "@next/env": "^15.1.6",
    "@types/formidable": "^3.4.5",
    "dayjs": "^1.11.10",
    "firebase": "^10.7.2",
    "formidable": "^3.5.2",
    "html2canvas": "^1.4.1",
    "next": "^15.1.6",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@percy/cli": "^1.27.4",
