    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "cypress": "^13.6.4",
    "cypress-file-upload": "^5.0.8",
    "cypress-firebase": "^3.0.2",
    "cypress-localstorage-commands": "^2.2.5",
